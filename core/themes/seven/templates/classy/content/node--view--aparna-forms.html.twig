{# <span class="newsletters_link"> Newsletters</span> || <span class="contactus_link"> Contact Us</span> || <span class="dealer_link"> Become a dealer</span> || <span class="careers_link"> Careers</span> #}

 <h4>Newsletters</h4>
<table class="newsletters table">
    <thead>
        <tr>
            <th class="text-center">#</th>
            <th class="text-center">Form Type</th>
            <th class="text-center">email</th>
        </tr>
    </thead>
    <tbody>
        {% for newsletter in newsletters %}
        <tr>
            <td>{{loop.index}}</td>
            <td>Newsletters</td>
            <td>{{newsletter.newsletter_email}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
<hr>
<br>  

<h4>Contact Us</h4>
<table class="contactus table">
    <thead>
        <tr>
            <th class="text-center">#</th>
            <th class="text-center">Form Type</th>
            <th class="text-center">First Name</th>
            <th class="text-center">Last Name</th>
            <th class="text-center">Email</th>
            <th class="text-center">Phone</th>
            <th class="text-center">Age</th>
            <th class="text-center">Pincode</th>
            <th class="text-center">Country</th>
            <th class="text-center">Project</th>
        </tr>
    </thead>
    <tbody>
        {% for contact in contactus %}
            <tr>
                <td>{{loop.index}}</td>
                <td>Contact Us</td>
                <td>{{contact.contactus_fname}}</td>
                <td>{{contact.contactus_lname}}</td>
                <td>{{contact.contactus_email}}</td>
                <td>{{contact.contactus_mobile}}</td>
                <td>{{contact.contactus_age}}</td>
                <td>{{contact.contactus_pincode}}</td>
                <td>{{contact.contactus_country}}</td>
                <td>{{contact.contactus_project}}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
<hr>
<br>
