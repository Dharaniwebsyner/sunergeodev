<h4>Careers</h4>
<table class="careers table">
    <thead>
        <tr>
            <th class="text-center">#</th>
            <th class="text-center">Form Type</th>
            <th class="text-center">First Name</th>
            <th class="text-center">Last Name</th>
            <th class="text-center">Email</th>
            <th class="text-center">Mobile</th>
            <th class="text-center">Alternate Mobile</th>
            <th class="text-center">Total Experience</th>
            <th class="text-center">Qualification</th>
            <th class="text-center">Current Location</th>
            {# <th class="text-center">Industry</th> #}
            <th class="text-center">Resume</th>
        </tr>
    </thead>
    <tbody>
        {% for career in careers %}
            <tr>
                <td>{{loop.index}}</td>
                <td>Careers</td>
                <td>{{career.carrers_fname}}</td>
                <td>{{career.carrers_lname}}</td>
                <td>{{career.carrers_email}}</td>
                <td>{{career.carrers_mobile}}</td>
                <td>{{career.carrers_mobilealt}}</td>
                <td>{{career.carrers_totalexperience}}</td>
                <td>{{career.carrers_qualification}}</td>
                <td>{{career.carrers_currentlocation}}</td>
                {# <td>{{career.carrers_industry}}</td> #}
                <td><a href="{{career.carrers_file}}" target="_blank"> Resume </a></td>
            </tr>
        {% endfor %}
    </tbody>
</table>


